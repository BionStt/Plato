@model EditTopicChannelsViewModel

<card>
    <card-title>
        <h6 class="card-title">
            @T["Channels"]
        </h6>
    </card-title>
    <card-body>

        @await DisplayAsync(new View("SelectChannel", new
        {
            selectedChannels = @Model.SelectedChannels,
            htmlName = @Model.HtmlName
        }))


    </card-body>
</card>
