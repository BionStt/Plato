@model Label

<div class="list-left">
    <span class="channel-icon channel-icon-md" style="background-color: @Model.BackColor; color: @Model.ForeColor">
        <i class="@Model.IconCss"></i>
    </span>
</div>
<div class="list-body">
    <h4>@Model.Name</h4>
    <span class="text-muted">
        @Model.Description
    </span>
</div>
<hr/>