@model Doc

<card class="card card-body-flush">
    <card-title>
        <h6 class="card-title">@T["Browse Docs"]</h6>
    </card-title>
    <card-body>
        @await DisplayAsync(new View("DocTree", new
        {
            options = new EntityTreeOptions()
            {
                SelectedEntity = Model.Id
            }
        }))
    </card-body>
</card>
