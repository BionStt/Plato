@using Plato.Internal.Layout.Views
@model EditUserRolesViewModel

<card>
    <card-title>
        <h6 class="card-title">@T["Roles"]</h6>
    </card-title>
    <card-body>
   

        @await DisplayAsync(new View("SelectRoles", new
        {
            selectedRoles = @Model.SelectedRoles,
            htmlName = @Model.HtmlName
        }))
        

    </card-body>
</card>

