@model Plato.Users.ViewModels.RegisterViewModel

@{
    ViewData["Title"] = T["Register"];
}

<h4>@ViewData["Title"]</h4>
<span class="text-muted">@T["Create a new account"]</span>
<div class="row">
    <div class="col-md-6">
        <hr />
        @await DisplayAsync(new View("RegisterForm", new
        {
            email = "",
            username = "",
            password = "",
            confirmPassword = ""
        }))
    </div>
</div>

