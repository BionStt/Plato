@model ResetPasswordViewModel
@{
    ViewData["Title"] = T["Reset Password"];
}

<h2>@ViewData["Title"]</h2>
<div class="row">
    <div class="col-md-6">
        <p>
            @T["Your password has been reset successfully."]
            <a asp-route-area="Plato.Users" asp-controller="Account" asp-route-action="Login">@T["Log in"]</a>
        </p>
    </div>
</div>