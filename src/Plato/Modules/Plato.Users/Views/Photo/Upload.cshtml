@model int
@{
    Layout = "";
    ViewData["Title"] = "Login";
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
</head>
<body>

    <form asp-controller="Photo" asp-action="Upload" asp-route-returnurl="@ViewData["ReturnUrl"]" method="post" enctype="multipart/form-data">

        <userphoto user-id="@Model" />

        <input type="file" name="file" />
        <br /><br />
        <input type="submit" value="Upload" />
    </form>
    
</body>
</html>
