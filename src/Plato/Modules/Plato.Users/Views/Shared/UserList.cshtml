@model UsersPaged

<h1>User List Partial View</h1>

<ul class="list-group">
    @foreach (var user in Model.Results.Data)
    {
        <li class="list-group-item">
            @user.UserName
        </li>
    }
</ul>

@Model.Results.Total

<pager page="@Model.PagerOpts.Page"
       page-size="@Model.PagerOpts.PageSize"
       total="@Model.Results.Total"></pager>


