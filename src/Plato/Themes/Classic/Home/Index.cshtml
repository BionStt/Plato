@model IEnumerable<object>
@{
    ViewData["Title"] = "Home Page";
}


<h2>Located Modules (from model)</h2>

<ul>
    @foreach (dynamic o in @Model)
    {
        <li>
            @o.Name - (@o.ID)
        </li>
        <br />
    }
</ul>

@await Component.InvokeAsync("Plato.Modules.Template", new { value = "test" } )

@await Component.InvokeAsync("Plato.Modules.ContentArea", new { value = "test" })

Display("Zone 1")


<br /><br />

<br /><br />

@ViewData["result"]

